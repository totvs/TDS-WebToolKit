{
  "name": "@totvs/tds-webtoolkit",
  "displayName": "TDS-WebToolkit",
  "description": "Standard UI for TDS products",
  "version": "0.0.20",
  "license": "Apache-2.0",
  "publisher": "TOTVS",
  "author": {
    "name": "TOTVS",
    "email": "tecnologia@totvs.com.br"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "npx -y rimraf -v dist",
    "--COMPILE--": "",
    "compile": "tsc",
    "watch": "tsc -watch",
    "copy-files": "npx -y copyfiles -u 1 src/**/*.css dist/",
    "start:vite": "vite",
    "-- BUILD --": "",
    "build": "npm run clean && webpack --mode=production",
    "---RELEASE---": "",
    "release": "npx -y standard-version",
    "tag": "git push --follow-tags",
    "to_publish": "npm publish",
    "prerelease": "npm run compile && npm run copy-files",
    "---MISC---": ""
  },
  "dependencies": {
    "@vscode-elements/elements": "^1.7.0",
    "@vscode-elements/react-elements": "^0.4.0",
    "@vscode/codicons": "^0.0.36",
    "react-dom": "^18.3.1",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/vscode": "^1.89.0",
    "@types/vscode-webview": "^1.57.5",
    "@vitejs/plugin-react": "^4.3.2",
    "@vscode-elements/webview-playground": "^1.1.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.4.8",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-stream": "^7.0.0"
  }
}
