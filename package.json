  "devDependencies": {
    "typescript-plugin-css-modules": "^5.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/vscode": "^1.91.0",
    "@types/vscode-webview": "^1.57.5",
    "css-loader": "^7.1.2",
